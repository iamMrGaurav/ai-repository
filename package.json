{
  "name": "ai-bunny",
  "version": "1.0.0",
  "description": "A Retrieval-Augmented Generation (RAG) system built with TypeScript, ChromaDB, and Google's Gemini AI",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun run rag_system.ts",
    "dev": "bun run index.ts",
    "reset": "bun run rag_system.ts --reset",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f chroma",
    "check-chroma": "curl http://localhost:8000/api/v1/heartbeat"
  },
  "keywords": [
    "rag",
    "chromadb",
    "langchain",
    "gemini",
    "typescript",
    "ai",
    "embeddings",
    "vector-database"
  ],
  "author": "Gaurav",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@chroma-core/default-embed": "^0.1.8",
    "@langchain/core": "^0.3.78",
    "@langchain/google-genai": "^0.2.18",
    "@langchain/textsplitters": "^0.1.0",
    "@types/express": "^5.0.3",
    "chromadb": "^3.0.17",
    "express": "^5.1.0",
    "install": "^0.13.0",
    "langchain": "^0.3.35",
    "zod": "^4.1.5"
  }
}
